.add {
	position: fixed;
	width: 84rpx;
	height: 84rpx;
	background: rgba(255, 255, 255, 1);
	border: 2px solid rgba(230, 230, 230, 1);
	-webkit-box-shadow: 0px 4rpx 16rpx 0px rgba(0, 0, 0, 0.08);
	        box-shadow: 0px 4rpx 16rpx 0px rgba(0, 0, 0, 0.08);
	border-radius: 50%;
	right: 5%;
	bottom: 100rpx;
}
.add image {
	width: 100%;
	height: 100%;
}
